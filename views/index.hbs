    <div class="">
        <h1 class="judul-project">MY PROJECT</h1>
    </div>


    {{!-- Halaman Project --}}

    <div class="card-group bg-bawah">

        {{#each dataProject}}
        <div class="card mx-5 shadow-lg rounded mb-3 p-3">
            <img src="/assets/images/images.jfif" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">
                    <a href="/project-detail/1" style="text-decoration: none; color:black"
                        target="_blank">{{this.name}}</a>
                </h5>
                <p class="card-text"><small class="text-muted">durasi: {{this.durasi}}</small></p>
                <p class="card-text"><small class="text-muted">{{this.author}}</small></p>
                <p class="card-text">{{this.description}}</p>

                {{#each technologies}}
                {{!-- <div class="fs-1"> --}}
                    <i class="fa-brands fa-{{this}} fs-1 me-5 mb-3"></i>
                    {{!-- <i class="fa-brands fa-{{this}}-js me-5"></i> --}}
                    {{!-- <i class="fa-brands fa-java me-5"></i> --}}
                    {{!-- <i class="fa-brands fa-python me-5"></i> --}}
                {{!-- </div> --}}
                {{/each}}

                <div class="d-flex mt-4">
                    <a href="/edit-project/{{@index}}" type="button" class="btn bg-dark text-white btn-sm me-3 edit">Edit</a>
                    {{!-- <a href="/delete-project/{{@index}}" type="button"
                        class="btn bg-dark text-white btn-sm ms-3 edit">Delete</a> --}}

                    <!-- Button trigger modal -->
                    <button type="button" class="btn bg-dark text-white btn-sm ms-3 edit" data-bs-toggle="modal" data-bs-target="#exampleModal">
                        Delete
                    </button>

                    <!-- Modal -->
                    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel"
                        aria-hidden="true">
                        <div class="modal-dialog">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="exampleModalLabel">Delete this project?</h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal"
                                        aria-label="Close"></button>
                                </div>
                                
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary"
                                        data-bs-dismiss="modal">Close</button>
                                    <a href="/delete-project/{{@index}}" type="button"
                                        class="btn bg-dark text-white">Delete</a>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
        {{/each}}
